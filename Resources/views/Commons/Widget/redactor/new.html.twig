<div id="widget-{{slot}}" class="add-widget">
{% block form %}
    <form method="POST" action="{{path('victoire_cms_widget_create', {'page': page.id,'slot':slot, 'type':'redactor', 'entity': entity} )}}">
        {% include "VictoireRedactorBundle:Commons:Widget/redactor/form.html.twig" %}

{% block submit %}
    <button class="btn btn-success" type="submit">{{ 'ok'|trans|desc('Ok') }}</button>
    <a class="btn" class="cancel" onclick="cancelWidget(this);">{{ 'cancel'|trans|desc('Annuler') }}</a>
{% endblock submit %}
    </form>
{% endblock form %}
<script type="text/javascript">
    $(document).ready(function(){
        initRedactor('.redactor');
    });
</script>
</div>
{% endblock body %}
